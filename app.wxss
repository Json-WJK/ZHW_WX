/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background:rgb(245,245,245);
}
navigator{
  display:inline-block;
  color:rgb(69, 151, 228);
}
text{
  display:inline-block;
}
/*灰色间隔分割线*/
.parting{
  height:25rpx;
  width:100%;
  background:rgb(245,245,245);
}
/*右侧小箭头*/
.arrow{  
  display: flex;  
  justify-content:space-between;  
  align-items: center;
  }
.arrow::after{  
  content:"";  
  width: 15rpx; 
  height: 15rpx; 
  border-top:3rpx solid #ccc; 
  border-right: 3rpx solid #ccc;  
  transform: rotate(45deg)
}